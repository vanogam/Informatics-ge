<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
    <persistence-unit name="default">
        <class>ge.freeuni.informatics.common.model.user.User</class>
        <class>ge.freeuni.informatics.common.model.user.RecoverPassword</class>
        <class>ge.freeuni.informatics.common.model.user.SolvedProblem</class>
        <class>ge.freeuni.informatics.common.model.contestroom.ContestRoom</class>
        <class>ge.freeuni.informatics.common.model.contest.Contest</class>
        <class>ge.freeuni.informatics.common.model.contest.ContestantResult</class>
        <class>ge.freeuni.informatics.common.model.task.Testcase</class>
        <class>ge.freeuni.informatics.common.model.task.Task</class>
        <class>ge.freeuni.informatics.common.model.post.Post</class>
        <class>ge.freeuni.informatics.common.model.post.PostComment</class>
        <class>ge.freeuni.informatics.common.model.submission.Submission</class>
        <class>ge.freeuni.informatics.common.model.worker.Worker</class>
        <properties>
            <!-- hibernate.hbm2ddl.auto is set from spring.jpa.hibernate.ddl-auto via DatasourceConfiguration -->
        </properties>
    </persistence-unit>
</persistence>